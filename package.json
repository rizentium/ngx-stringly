  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rizentium/ngx-stringly/issues"
  },
  "homepage": "https://github.com/rizentium/ngx-stringly#readme",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  },
  "files": [
    "lib/**/*"
